<UserControl x:Class="AlertLED.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AlertLED"
             mc:Ignorable="d" 
             d:DesignHeight="170" d:DesignWidth="160">
    <Grid Margin="0,0,0,0">
        <Grid x:Name="grdLED">
            <Ellipse x:Name="ellipseBracket" HorizontalAlignment="Left" Height="160" Margin="0,-1,0,0" VerticalAlignment="Top" Width="155.4" Stroke="#FF232323">
                <Ellipse.Fill>
                    <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                        <LinearGradientBrush.RelativeTransform>
                            <TransformGroup>
                                <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                                <SkewTransform CenterY="0.5" CenterX="0.5"/>
                                <RotateTransform Angle="14" CenterY="0.5" CenterX="0.5"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </LinearGradientBrush.RelativeTransform>
                        <GradientStop Color="White" Offset="0.279"/>
                        <GradientStop Color="#FF838282"/>
                        <GradientStop Color="White" Offset="0.341"/>
                        <GradientStop Color="#FF999898" Offset="0.49"/>
                        <GradientStop Color="#FF999898" Offset="0.652"/>
                        <GradientStop Color="#FFF0F0F0" Offset="0.723"/>
                        <GradientStop Color="#FFF0F0F0" Offset="0.846"/>
                        <GradientStop Color="#FF868686" Offset="0.98"/>
                    </LinearGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse x:Name="ellipseLEDBagground" HorizontalAlignment="Left" Height="109.6" Margin="24,23.638,0,0" Stroke="Black"  VerticalAlignment="Top" Width="106" Fill="Black"/>
            <Ellipse x:Name="ellipseLED" HorizontalAlignment="Left" Height="109.6" Margin="24.5,23.638,0,0" Stroke="Black" VerticalAlignment="Top" Width="106" StrokeThickness="0">
                <Ellipse.Fill>
                    <RadialGradientBrush>
                        <GradientStop Color="#FF8C0000" Offset="1"/>
                        <GradientStop Color="Red" Offset="0.819"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>

            <Ellipse x:Name="elipseHotSpot" HorizontalAlignment="Left" Height="18.4" Margin="91.7,44.4,0,0" VerticalAlignment="Top" Width="17.6">
                <Ellipse.Fill>
                    <RadialGradientBrush>
                        <GradientStop Color="#FFFCFCFC" Offset="0.517"/>
                        <GradientStop Color="Red" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Image x:Name="imgUnbraco_Copy" HorizontalAlignment="Left" Height="11.6" Margin="138.15,197.45,-5.95,-49.05" VerticalAlignment="Top" Width="11.6" Source="Unbraco.png"/>



        </Grid>

        <Image x:Name="imgUnbraco" HorizontalAlignment="Left" Height="11.6" Margin="73.25,127.45,0,0" VerticalAlignment="Top" Width="11.6" Source="GFX/Unbraco.png"/>
        <Image x:Name="imgUnbraco2" HorizontalAlignment="Left" Height="11.6" Margin="137.15,197.05,0,0" VerticalAlignment="Top" Width="11.6" Source="GFX/Unbraco.png"/>
        <Image x:Name="imgUnbraco3" HorizontalAlignment="Left" Height="11.6" Margin="73.25,260.55,0,0" VerticalAlignment="Top" Width="11.6" Source="GFX/Unbraco.png"/>
        <Image x:Name="imgUnbraco4" HorizontalAlignment="Left" Height="11.6" Margin="8.65,197.05,0,0" VerticalAlignment="Top" Width="11.6" Source="GFX/Unbraco.png"/>
        <Ellipse Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="100" Margin="-540.178,121.23,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>

    </Grid>
</UserControl>
