<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="HEAVI.ShadowTextBlock"
	x:Name="UserControl" FontSize="48" Height="57.63" Width="134.363">

    <Grid x:Name="LayoutRoot" Height="500">
        <TextBlock x:Name="textBlockMainShadow" TextAlignment="Center" Height="{Binding Height, ElementName=UserControl}" FontSize="{Binding FontSize, ElementName=UserControl}" Foreground="Black" TextWrapping="Wrap" FontWeight="Black" Text="mWh" Margin="0" Width="{Binding Width, ElementName=UserControl}" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlockMain"       TextAlignment="Center" Height="{Binding Height, ElementName=UserControl}" FontSize="{Binding FontSize, ElementName=UserControl}" Foreground="{Binding Foreground, ElementName=UserControl}" TextWrapping="Wrap" FontWeight="Black" Text="mWh" Margin="2,2,0,0" Width="{Binding Width, ElementName=UserControl}" VerticalAlignment="Top"/>
	</Grid>
</UserControl>